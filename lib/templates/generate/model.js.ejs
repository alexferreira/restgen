module.exports.<%= objectName %> = function (mongoose) {
  var Schema = mongoose.Schema;

  /* schema for <%= objectName %> */
  <%= objectName %>Schema = new Schema ({<% if (fields) { %><% fields.forEach(function(field){ %>
    <%= field.name %>: <%= field.type %>,<% }) %> <% } else { %> 
    textValue: String <% } %>
  }, { versionKey: false });

  /* methods for <%= objectName %>Schema */

  // <%= objectName %>Schema.method({
  //   example: function () {
  //     return true;
  //   }
  // });

  return mongoose.model('<%= objectName %>', <%= objectName %>Schema);
};
