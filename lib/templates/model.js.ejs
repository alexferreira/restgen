module.exports.<%= modelName %> = function (mongoose) {
  var Schema = mongoose.Schema;

  /* schema for <%= modelName %> */
  <%= modelName %>Schema = new Schema ({
    textValue: String
  }, { versionKey: false });

  /* methods for <%= modelName %>Schema */

  // <%= modelName %>Schema.method({
  //   example: function () {
  //     return true;
  //   }
  // });

  return mongoose.model('<%= modelName %>', <%= modelName %>Schema);
};
